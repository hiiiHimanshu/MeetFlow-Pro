# Core
NODE_ENV=development
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/meetings
REDIS_URL=redis://localhost:6379

# S3 compatible storage (MinIO/Cloudflare R2/S3)
S3_ENDPOINT=http://localhost:9000
S3_REGION=us-east-1
S3_ACCESS_KEY_ID=minioadmin
S3_SECRET_ACCESS_KEY=minioadmin
S3_BUCKET=meetings

# Pinecone & LLM
PINECONE_API_KEY=your-pinecone
PINECONE_INDEX=meetings
OPENAI_API_KEY=your-openai
LLM_PROVIDER=openai
TEMPERATURE=0.2

# Auth
AUTH_SECRET=dev-secret
GOOGLE_CLIENT_ID=your-google
GOOGLE_CLIENT_SECRET=your-google-secret

# Integrations
SLACK_BOT_TOKEN=xoxb-...
SLACK_SIGNING_SECRET=...
NOTION_TOKEN=secret_...

# Observability
SENTRY_DSN=
